export SERVICE_POSTGRES_NAME=postgres

export SERVICE_POSTGRES_VERSION=9.5-alpine
export SERVICE_POSTGRES_CONFIG=$SERVICE_CONFIG

export SERVICE_POSTGRES_HOST=0.0.0.0
export SERVICE_POSTGRES_PORT=5432

export SERVICE_POSTGRES_USERNAME=
export SERVICE_POSTGRES_PASSWORD=
export SERVICE_POSTGRES_DB=

export SERVICE_POSTGRES_RESOURCE_LIMIT_CPU=${SERVICE_POSTGRES_RESOURCE_LIMIT_CPU:-4}
export SERVICE_POSTGRES_RESOURCE_LIMIT_MEM=${SERVICE_POSTGRES_RESOURCE_LIMIT_MEM:-8192m}

if [ ! -f $SERVICE_POSTGRES_CONFIG ]; then
    touch $SERVICE_POSTGRES_CONFIG
fi
